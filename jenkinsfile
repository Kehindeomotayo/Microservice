pipeline {
    agent any

    stages {
        stage('Deploy To Kubenetes') {
            steps {
                withKubeCredentials(kubectlCredentials: [[caCertificate: '', clusterName: ' EKS1', contextName: '', credentialsId: 'kennie2-token', namespace: 'webapps', serverUrl: '']]) {
                    sh "kubectl apply-f deployment-service.yml"
                    
               }
            }
        }
        
        stage('verify Deployment') {
            steps {
                withKubeCredentials(kubectlCredentials: [[caCertificate: '', clusterName: ' EKS1', contextName: '', credentialsId: 'kennie2-token', namespace: 'webapps', serverUrl: '']]) {
        
        }         sh "kubectl get svc -n webapps"
            }
        }
    }
}
